<div class="container">
  <div class="row">
    <div class="col-md-8">
      <% @enterprises.each do |enterprise| %>
          <%= render partial: 'enterprise', locals: {enterprise: enterprise} %>
      <% end %>

      <%= render partial: 'pager' %>

    </div>
    <%= render partial: 'sidebar' %>
  </div>
  <div class="text-center"><%= will_paginate @enterprises, renderer: BootstrapPagination::Rails %></div>
</div>