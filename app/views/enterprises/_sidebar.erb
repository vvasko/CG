<!-- Blog Sidebar Widgets Column -->
<div class="col-md-4">

  <%= form_tag(enterprises_path, :method => "get", id: "search-form") do %>
  <div class="well">
    <h4><%=t 'Enterprise Search' %></h4>
    <div class="input-group">
      <%= text_field_tag :search, params[:search], class: "form-control", placeholder: "Search Enterprises" %>
        <span class="input-group-btn">
          <button class="btn btn-default" type="submit">
            <span class="glyphicon glyphicon-search"></span>
          </button>
        </span>
    </div>
  </div>
  <% end %>

